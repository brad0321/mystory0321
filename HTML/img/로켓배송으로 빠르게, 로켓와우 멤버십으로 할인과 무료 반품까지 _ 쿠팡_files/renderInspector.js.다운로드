define(function(){var renderInspector=window.webInspectorManager?window.webInspectorManager.renderInspector:null;var GNB_RENDER_FAILED="gnb_render_failed";var TODAYS_HOT_RENDER_FAILED="todays_hot_render_failed";var TODAY_DISCOVERY_UNIT_RENDER_FAILED="today_discovery_unit_render_failed";var FULL_GNB_HEADER="#full-gnb-header";var TODAYS_HOT="#todaysHot";var MAIN_TODAY_SELECTED_PRODUCT=".main-today__selected-product";var TODAY_DISCOVERY_HOT="#todayDiscoveryUnit";var TODAY_DISCOVERY=".today-discovery";
var MAJOR="major";return{assertGnb:function(){if(renderInspector)renderInspector.assert(GNB_RENDER_FAILED,[FULL_GNB_HEADER])},assertTodaysHot:function(){if(renderInspector)renderInspector.assert(TODAYS_HOT_RENDER_FAILED,[MAIN_TODAY_SELECTED_PRODUCT],{parent:TODAYS_HOT,severity:MAJOR})},assertTodayDiscoveryUnit:function(){if(renderInspector)renderInspector.assert(TODAY_DISCOVERY_UNIT_RENDER_FAILED,[TODAY_DISCOVERY],{parent:TODAY_DISCOVERY_HOT,severity:MAJOR})}}});